#include <stdio.h>

int main() {
    int i, j, n, s;

    // Prompt the user to enter the value of 'n'
    printf("Enter the n value: ");
    scanf("%d", &n);

    for (i = 0; i < n; i++) {
        for (j = 0; j < n; j++) {
            // Calculate the sum of i and j +1
            s = i + j +1 ;

            // Check the range of the sum and print accordingly
            if (s >= 0 && s <= 9)
                printf("0%d ", s);
            else
                printf("%d ", s);
        }

        // Print a new line after each inner loop iteration
        printf("\n");
    }

    return 0;
}
