#include <stdio.h>

int main() {
    int i, j, n, s;

    // Prompt the user to enter the value of 'n'
    printf("Enter the n value: ");
    scanf("%d", &n);

    for (i = 0; i < n; i++) {
        for (j = 0; j < n; j++) {
            // Print asterisk (*) followed by two spaces
            printf("*  ");
        }
        // Move to the next line after printing 'n' asterisks in the inner loop
        printf("\n");
    }

    return 0;
}
